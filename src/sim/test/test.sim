let loop = fn(x, y, z) {
  if (x < (x + y) - 1) {
    let y = y - 1
    let x = x + 1;
    let z = z + z * x
    println(x)
    println(z)
    loop(x, y, z)
  } else {
    println(y + x)
    return "END"
  }
};

